<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="StartOutOfProcess" value="false"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <runtime>
    <ThrowUnobservedTaskExceptions enabled="true"/>
    <gcServer enabled="true"/>
    <gcConcurrent enabled="false"/>
  </runtime>
</configuration>
