    <PostBuildEvent>
      @echo UnitTest - Start Post-Build script

      if not exist "$(TargetDir)TestInput" ( mkdir "$(TargetDir)TestInput" )

      REM copy /y "$(ProjectDir)test.config"  "$(TargetDir)TestInput\"
      copy /y "$(ProjectDir)\*.xml"  "$(TargetDir)TestInput\"
      copy /y "$(SolutionDir)OrleansMgmtUI\OrleansManager\Deployment.xml"  "$(TargetDir)TestInput\"

      @REM Ensure Azure RoleEnvironment.IsAvailable call does not complain about missing msshrtmi.dll
      REM copy /y "$(TargetDir)\*.dll"  "$(TargetDir)TestInput\"
      REM copy /y "$(SolutionDir)Dependencies\Windows-Azure-SDK\v$(AzureSdkVersion)\bin\runtimes\base\x64\msshrtmi.dll"  "$(TargetDir)TestInput\"

      @echo Deployment Tests: copying to $(TargetDir)TestInput\Configuration
      if not exist "$(TargetDir)TestInput\Configuration" ( mkdir "$(TargetDir)TestInput\Configuration" )
      if not exist "$(TargetDir)TestInput\Configuration\TestAbsolutePaths" ( mkdir "$(TargetDir)TestInput\Configuration\TestAbsolutePaths" )
      if not exist "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans" ( mkdir "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans")
      if not exist "$(TargetDir)TestInput\Configuration\TestCleanOrleans" ( mkdir "$(TargetDir)TestInput\Configuration\TestCleanOrleans")
      if not exist "$(TargetDir)TestInput\Configuration\TestRelativePaths" ( mkdir "$(TargetDir)TestInput\Configuration\TestRelativePaths")

      copy /y "$(SolutionDir)Scripts\Deployment\*.ps1"  "$(TargetDir)TestInput\"
      copy /y "$(ProjectDir)\DeploymentTests\Configuration\TestAbsolutePaths\*.*"  "$(TargetDir)TestInput\Configuration\TestAbsolutePaths"
      copy /y "$(ProjectDir)\DeploymentTests\Configuration\TestCleanEmptyOrleans\*.*"  "$(TargetDir)TestInput\Configuration\TestCleanEmptyOrleans"
      copy /y "$(ProjectDir)\DeploymentTests\Configuration\TestCleanOrleans\*.*"  "$(TargetDir)TestInput\Configuration\TestCleanOrleans"
      copy /y "$(ProjectDir)\DeploymentTests\Configuration\TestRelativePaths\*.*"  "$(TargetDir)TestInput\Configuration\TestRelativePaths"

      @echo UnitTests: copying to $(TargetDir)TestInput
      if not exist "$(TargetDir)TestInput\Applications" ( mkdir "$(TargetDir)TestInput\Applications" )
      if not exist "$(TargetDir)TestInput\Applications\UnitTests" ( mkdir "$(TargetDir)TestInput\Applications\UnitTests" )

rem @echo UnitTests: copying test image files to $(TargetDir)TestInput
rem copy /y "$(TargetDir)*.png"  "$(TargetDir)TestInput\"

      @echo UnitTests: copying test Db files to $(TargetDir)TestInput
      copy /y "$(TargetDir)Data\TestDb*.*"  "$(TargetDir)TestInput\"
      attrib -r "$(TargetDir)TestInput\TestDb*"

      if "$(BuildingInsideVisualStudio)" == "true" (
      copy /y "$(ProjectDir)..\UnitTestGrains\bin\$(ConfigurationName)\UnitTestGrains.*"  "$(TargetDir)TestInput\"
      copy /y "$(ProjectDir)..\UnitTestGrainInterfaces\bin\$(ConfigurationName)\UnitTestGrainInterfaces.*"  "$(TargetDir)TestInput\"
      ) else (
      copy /y "$(TargetDir)\UnitTestGrains.*"  "$(TargetDir)TestInput\"
      copy /y "$(TargetDir)\UnitTestGrainInterfaces.*"  "$(TargetDir)TestInput\"
      )

      @echo UnitTest - End Post-Build script
