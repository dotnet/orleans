) else (
set IN_DIR=$(OutDir)
)

set D=%25TEST_SILO_DIR%25
if not exist "%25D%25" (md "%25D%25") else (del /s/q "%25D%25\*")

@ECHO Copying references from build - $(SolutionDir)..\SampleReferenceFiles.txt
for /f "usebackq tokens=1,2" %25%25i in ("$(SolutionDir)..\SampleReferenceFiles.txt") do (
  @ECHO "%25IN_DIR%25%25%25i"
  copy /y "%25IN_DIR%25%25%25i" "%25TEST_SILO_DIR%25%25%25j"
)</PreBuildEvent>
