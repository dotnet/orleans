<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="NuGet">
    <PackageId>Microsoft.Orleans.OrleansGoogleUtils</PackageId>
    <Title>Microsoft Orleans Google Utilities</Title>
    <Description>
      Library of utility types for Google of Microsoft Orleans.
    </Description>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+win8+wpa81+wp8</PackageTargetFallback>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyName>OrleansGoogleUtils</AssemblyName>
    <RootNamespace>OrleansGoogleUtils</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Orleans\Orleans.2017.csproj" />
    <ProjectReference Include="..\OrleansRuntime\OrleansRuntime.2017.csproj" />
  </ItemGroup>
</Project>