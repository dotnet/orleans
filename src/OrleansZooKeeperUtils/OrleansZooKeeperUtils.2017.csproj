<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="NuGet">
    <IsPackable>true</IsPackable>
    <PackageId>Microsoft.Orleans.OrleansZooKeeperUtils</PackageId>
    <Title>Microsoft Orleans ZooKeeper Utilities</Title>
    <Description>
      Library of utility types for ZooKeeper of Microsoft Orleans.
    </Description>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net451</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+win8+wpa81+wp8</PackageTargetFallback>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <NoWarn>1591</NoWarn>
    <AssemblyName>OrleansZooKeeperUtils</AssemblyName>
    <RootNamespace>OrleansZooKeeperUtils</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\Build\GlobalAssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ZooKeeperNetEx" Version="3.4.8.5" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Orleans\Orleans.2017.csproj" />
  </ItemGroup>
</Project>