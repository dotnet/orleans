{

  "build": {
    "content": [
      {
        "files": [
          "Documentation/**/**.yml",
          "Documentation/**/**.md",
          "Tutorials/**.yml",
          "Tutorials/**.md",
          "Community/**.yml",
          "Community/**.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.svg",
          "logo-light.svg"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [ ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../",
    "template": [
      "default", "template"
    ],
    "noLangKeyword": false,
    "globalMetadata": {
      "_appTitle": "Microsoft Orleans Website",
      "_enableSearch": true
    }
  }

}
