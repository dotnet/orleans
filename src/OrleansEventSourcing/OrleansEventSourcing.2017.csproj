<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="NuGet">
    <PackageId>Microsoft.Orleans.EventSourcing</PackageId>
    <Title>Microsoft Orleans Event-Sourcing</Title>
    <Description>
      Base types for creating Microsoft Orleans grains with event-sourced state.
    </Description>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyName>OrleansEventSourcing</AssemblyName>
    <RootNamespace>OrleansEventSourcing</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Orleans\Orleans.2017.csproj" />
  </ItemGroup>
</Project>
