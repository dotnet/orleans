<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Microsoft.Orleans.OrleansTelemetryConsumers.Linux</PackageId>
    <Title>Microsoft Orleans Telemetry Consumer - Linux</Title>
    <Description>Microsoft Linux implementation of Orleans Telemetry API.</Description>
    <TargetFrameworks>$(StandardTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>Orleans.TelemetryConsumers.Linux</AssemblyName>
    <RootNamespace>OrleansTelemetryConsumers.Linux</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="$(MicrosoftExtensionsLoggingVersion)" />
  </ItemGroup>
</Project>
