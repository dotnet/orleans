<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Microsoft.Orleans.OrleansTelemetryConsumers.AI</PackageId>
    <Title>Microsoft Orleans Telemetry Consumer - Microsoft Azure Application Insights</Title>
    <Description>Microsoft Azure Application Insights implementation of Orleans Telemetry API.</Description>
    <PackageTags>$(PackageTags) Azure ApplicationInsights</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>Orleans.TelemetryConsumers.AI</AssemblyName>
    <RootNamespace>OrleansTelemetryConsumers.AI</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ApplicationInsights" Version="$(MicrosoftApplicationInsightsVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Orleans.Core.Abstractions\Orleans.Core.Abstractions.csproj" />
    <ProjectReference Include="..\Orleans.Core\Orleans.Core.csproj" />
    <ProjectReference Include="..\Orleans.Runtime.Abstractions\Orleans.Runtime.Abstractions.csproj" />
  </ItemGroup>
</Project>
