<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Microsoft.Orleans.Streaming.GooglePubSub</PackageId>
    <Title>Microsoft Orleans Google PubSub Streaming Provider</Title>
    <Description>Microsoft Orleans streaming provider backed by Google PubSub</Description>
    <PackageTags>$(PackageTags) Google PubSub</PackageTags>
    <TargetFrameworks>$(DefaultTargetFrameworks)</TargetFrameworks>
    <AssemblyName>Orleans.Streaming.GooglePubSub</AssemblyName>
    <RootNamespace>Orleans.Streaming.GooglePubSub</RootNamespace>
    <OrleansBuildTimeCodeGen>true</OrleansBuildTimeCodeGen>
    <DefineConstants>$(DefineConstants);ORLEANS_STREAMING</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(SourceRoot)src\Orleans.Streaming\Orleans.Streaming.csproj" />
    <PackageReference Include="Google.Cloud.PubSub.V1" />
    <PackageReference Include="System.Net.NameResolution" />
  </ItemGroup>
</Project>
