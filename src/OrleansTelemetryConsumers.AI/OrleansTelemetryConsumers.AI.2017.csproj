<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="NuGet">
    <IsPackable>true</IsPackable>
    <PackageId>Microsoft.Orleans.OrleansTelemetryConsumers.AI</PackageId>
    <Title>Microsoft Orleans Telemetry Consumer - Microsoft Azure Application Insights</Title>
    <Description>
      Microsoft Azure Application Insights implementation of Orleans Telemetry API.
    </Description>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net451</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+win8+wpa81+wp8</PackageTargetFallback>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <NoWarn>1591</NoWarn>
    <AssemblyName>OrleansTelemetryConsumers.AI</AssemblyName>
    <RootNamespace>OrleansTelemetryConsumers.AI</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\Build\GlobalAssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ApplicationInsights" Version="1.2.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Orleans\Orleans.2017.csproj" />
  </ItemGroup>
</Project>