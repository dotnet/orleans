<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="NuGet">
    <PackageId>Microsoft.Orleans.OrleansTelemetryConsumers.AI</PackageId>
    <Title>Microsoft Orleans Telemetry Consumer - Microsoft Azure Application Insights</Title>
    <Description>
      Microsoft Azure Application Insights implementation of Orleans Telemetry API.
    </Description>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyName>OrleansTelemetryConsumers.AI</AssemblyName>
    <RootNamespace>OrleansTelemetryConsumers.AI</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ApplicationInsights" Version="1.2.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Orleans\Orleans.2017.csproj" />
  </ItemGroup>
</Project>
